Notebook[{
Cell[BoxData[
    \(x[\[Theta]_, r_, t_] := 
      x0 \((1 - t)\)\^3 + 3\ x1[r] \(\((1 - t)\)\^2\) t + 
        3\ x2[\[Theta], r] \((1 - t)\) t\^2 + x3[\[Theta]]\ t\^3\)], "Input"],

Cell[BoxData[
    \(y[\[Theta]_, r_, t_] := 
      y0 \((1 - t)\)\^3 + 3\ y1[r] \(\((1 - t)\)\^2\) t + 
        3\ y2[\[Theta], r] \((1 - t)\) t\^2 + y3[\[Theta]]\ t\^3\)], "Input"],

Cell[BoxData[
    \(vx[t_] := \[PartialD]\_s\ x[s] /. s \[Rule] t\)], "Input"],

Cell[BoxData[
    \(vy[t_] := \[PartialD]\_s\ y[s] /. s \[Rule] t\)], "Input"],

Cell[CellGroupData[{

Cell["Mean value integral", "Section"],

Cell["The goal is to evaluate", "Text"],

Cell[BoxData[
    \(\[Integral]\_0\%1 x[
          t] \(\@\(vx[t]\^2 + vy[t]\^2\)\) \[DifferentialD]t\)], "Input"],

Cell["and similarly for y.", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
    \(Collect[vx[t], {x0, x1, x2, x3}]\)], "Input"],

Cell[BoxData[
    \(\(-3\)\ \((1 - t)\)\^2\ x0 + \((3\ \((1 - t)\)\^2 - 
            6\ \((1 - t)\)\ t)\)\ x1 + \((6\ \((1 - t)\)\ t - 
            3\ t\^2)\)\ x2 + 3\ t\^2\ x3\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(Collect[
      Limit[\(1\/t\) vx[t] /. x1 \[Rule] x0, t \[Rule] 0], {x0, x1, x2, 
        x3}]\)], "Input"],

Cell[BoxData[
    \(\(-6\)\ x0 + 6\ x2\)], "Output"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Circular arc", "Section"],

Cell[BoxData[
    \(Clear[x0, y0, x1, y1, x2, y2, x3, y3]\)], "Input"],

Cell[BoxData[
    \(x0 := \ 1\)], "Input"],

Cell[BoxData[
    \(y0 := \ 0\)], "Input"],

Cell[BoxData[
    \(x3[\[Theta]_] := Cos[\[Theta]]\)], "Input"],

Cell[BoxData[
    \(y3[\[Theta]_] := Sin[\[Theta]]\)], "Input"],

Cell[BoxData[
    \(x1[r_] := x0\)], "Input"],

Cell[BoxData[
    \(y1[r_] := y0 + r\)], "Input"],

Cell[BoxData[
    \(x2[\[Theta]_, r_] := x3[\[Theta]] + Sin[\[Theta]] r\)], "Input"],

Cell[BoxData[
    \(y2[\[Theta]_, r_] := y3[\[Theta]] - Cos[\[Theta]]\ r\)], "Input"],

Cell[BoxData[
    \(x[\[Theta]_, r_, t_] := 
      x0 \((1 - t)\)\^3 + 3\ x1[r] \(\((1 - t)\)\^2\) t + 
        3\ x2[\[Theta], r] \((1 - t)\) t\^2 + x3[\[Theta]]\ t\^3\)], "Input"],

Cell[BoxData[
    \(y[\[Theta]_, r_, t_] := 
      y0 \((1 - t)\)\^3 + 3\ y1[r] \(\((1 - t)\)\^2\) t + 
        3\ y2[\[Theta], r] \((1 - t)\) t\^2 + y3[\[Theta]]\ t\^3\)], "Input"],

Cell[TextData[{
  "The square of the radius, as a function of ",
  Cell[BoxData[
      \(TraditionalForm\`t\)]],
  " is"
}], "Text"],

Cell[BoxData[
    \(R[\[Theta]_, r_, t_] := 
      x[\[Theta], r, t]\^2 + y[\[Theta], r, t]\^2\)], "Input"],

Cell[BoxData[
    \(dR[\[Theta]_, r_, t_] := \ \[PartialD]\_s\ R[\[Theta], r, s] /. 
        s \[Rule] t\)], "Input"],

Cell[CellGroupData[{

