[ toten :: [\ start] [if start > 10 [list] [cons start [toten start+1]]] ]

[ for_each :: [\ fun lst] [foldl [\ h a]{[fun h] a} VOID lst] ]

[ map :: [\ fun lst] [foldr [\ h a][cons [fun h] a] [list] lst] ]

[for_each show [map [(+) 2 ...] [toten 5]]]

[for_each show [list `a´ 1 [scale 3] `b´]]

[stroke (0cm,0cm)--(1cm,1cm)]
